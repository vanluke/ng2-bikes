{
  "name": "ng2-bikes",
  "version": "1.0.0",
  "description": "Bikes app",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/jest",
    "test:watch": "node_modules/.bin/jest --watch",
    "typings": "node_modules/.bin/typings install",
    "test:ci": "jest --runInBand",
    "start": "npm run start:dev",
    "start:dev": "node_modules/.bin/webpack-dev-server --port=3000 --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanluke/ng2-bikes.git"
  },
  "keywords": [
    "Angular2",
    "RxJS",
    "Redux",
    "Sass"
  ],
  "author": "Lukasz Gonciarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanluke/ng2-bikes/issues"
  },
  "homepage": "https://github.com/vanluke/ng2-bikes#readme",
  "jest": {
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/client/jest-style-mock.js"
    },
    "globals": {
      "__TS_CONFIG__": "<rootDir>/tsconfig.json"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.js$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/client/setup-jest.ts",
    "testRegex": "(\\**.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-preset-angular": "0.0.13",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "reflect-metadata": "^0.1.10",
    "regenerator-runtime": "^0.10.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "ts-helpers": "^1.1.2",
    "ts-jest": "^19.0.8",
    "ts-loader": "^2.0.3",
    "tslint-config-airbnb": "^1.1.1",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.2.2",
    "typings": "^2.1.0",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "zone.js": "^0.8.5"
  },
  "dependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "angularfire2": "^2.0.0-beta.8",
    "firebase": "^3.7.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.2.1-smooth",
    "seamless-immutable": "^7.1.2"
  }
}
